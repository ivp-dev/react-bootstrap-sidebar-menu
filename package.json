{
  "name": "react-bootstrap-sidebar-menu",
  "version": "0.1.0",
  "description": "Sidebar menu based on bootstrap",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "types": "types/index.d.ts",
  "author": "ivp.dev",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "start": "npm run start --prefix www",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd www && npm install && npm run build",
    "deploy": "gh-pages -d www/build",
    "build": "rollup -c -w",
    "build:types": "tsc -d --outDir types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivp-dev/react-bootstrap-sidebar-menu.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "sidebar",
    "menu"
  ],
  "bugs": {
    "url": "https://github.com/ivp-dev/react-bootstrap-sidebar-menu/issues"
  },
  "homepage": "https://github.com/ivp-dev/react-bootstrap-sidebar-menu#readme",
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "rollup": "^2.52.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "react-bootstrap": "^1.6.1"
  }
}
